#include <iostream>
using namespace std;

int main(){
	int i,j,k,n;
	
	cout << "Enter the size of the array : ";
	cin >> n;
	
	int arr1[n],arr2[n],arr3[n];
	cout << "Enter element of 1st array : ";
	for(i=0;i<n;i++){
		cin >> arr1[i];
	}	
	cout << "Enter element of 2nd array : ";
	for(j=0;j<n;j++){
		cin >> arr2[j];
	}
	cout << "Sum of 1st and 2nd array element are : ";
	for(k=0;k<n;k++){
		arr3[k]=arr1[k]+arr2[k];
		cout << " " << arr3[k];	
	}
	
	
	return 0;
}
